<div class="new_user">
	<h1 class="new_user">New User</h1>
	<%= form_for :user, url: users_path do |f| %>
	  <p>
		<%= f.label :email %><br>
		<%= f.text_field :email %>
	  </p>
	  
	  <p>
		<%= f.label :username %><br>
		<%= f.text_field :username %>
	  </p>
	 
	  <p>
		<%= f.label :password %><br>
		<%= f.password_field :password %>
	  </p>
	  
	  <p>
		<%= f.submit %>
	  </p>
	<% end %>
</div>
