<div class="form-container">
	<h1 class="form-title">Login</h1>
	<%= form_for :user, url: users_path do |f| %>
	  <%= debug params %>
	  <p>
		<%= f.label :username %><br>
		<%= f.text_field :username %>
	  </p>
	 
	  <p>
		<%= f.label :password %><br>
		<%= f.password_field :password %>
	  </p>
	  
	  <p>
		<%= f.submit "Login" %>
	  </p>
	<% end %>
	<br/>
</div>